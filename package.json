{
  "name": "qualitywatcher",
  "description": "Reporting node module for javascript tests",
  "main": "index.js",
  "version": "0.0.0",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 30 --branches 30 --functions 30 --lines 30",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha test/sendToQualitywatcher.js -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QualityWorksCG/qualitywatcher.git"
  },
  "keywords": [
    "nightwatch",
    "webdriver",
    "javascript",
    "test",
    "report"
  ],
  "author": "QualityWorks Consulting Group Jamaica (http://qualityworkscg.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QualityWorksCG/qualitywatcher/issues"
  },
  "homepage": "https://github.com/QualityWorksCG/qualitywatcher#readme",
  "devDependencies": {
    "chai": "3.4.0",
    "commitizen": "2.3.0",
    "coveralls": "2.11.4",
    "cz-conventional-changelog": "1.1.4",
    "ghooks": "0.3.2",
    "istanbul": "0.4.0",
    "mocha": "2.3.3",
    "mocha-lcov-reporter": "1.0.0",
    "semantic-release": "^4.3.5",
    "should": "7.1.1",
    "should-http": "0.0.4",
    "sinon": "1.17.2",
    "supertest": "1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  },
  "dependencies": {
    "app-root-path": "1.0.0",
    "dotenv": "1.2.0",
    "fs": "0.0.2",
    "js-yaml": "3.4.3",
    "jsonfile": "2.2.3",
    "path": "0.12.7",
    "request": "2.65.0"
  }
}
