{
  "name": "qualitywatcher",
  "description": "Reporting node module for javascript tests",
  "main": "dist/index.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "pretest": "jshint lib",
    "test": "nyc --require babel-core/register _mocha -- test/sendToQualitywatcher.js",
    "cover": "npm t && nyc report --reporter=text-lcov | coveralls",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:lib": "babel --out-dir dist lib",
    "build:bin": "babel --out-dir dist/bin bin",
    "postbuild": "npm install . -g",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "qw_report": "nightwatch | qualitywatcher"
  },
  "bin": {
    "qualitywatcher": "dist/bin/qualitywatcher.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QualityWorksCG/qualitywatcher.git"
  },
  "keywords": [
    "nightwatch",
    "webdriver",
    "javascript",
    "test",
    "report"
  ],
  "files": [
    "dist",
    "test",
    "README.md"
  ],
  "author": "QualityWorks Consulting Group Jamaica (http://qualityworkscg.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QualityWorksCG/qualitywatcher/issues"
  },
  "homepage": "https://github.com/QualityWorksCG/qualitywatcher#readme",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "chai": "3.4.0",
    "commitizen": "2.8.2",
    "coveralls": "2.11.4",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "0.3.2",
    "istanbul": "0.4.0",
    "jshint": "2.9.2",
    "mocha": "2.3.3",
    "mocha-lcov-reporter": "1.0.0",
    "npm-run-all": "2.3.0",
    "nyc": "7.0.0",
    "rimraf": "2.5.3",
    "semantic-release": "4.3.5",
    "should": "7.1.1",
    "should-http": "0.0.4",
    "sinon": "1.17.2",
    "supertest": "1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "dependencies": {
    "app-root-path": "1.0.0",
    "chalk": "1.1.3",
    "clear": "0.0.1",
    "clui": "0.3.1",
    "dotenv": "1.2.0",
    "figlet": "1.1.2",
    "fs": "0.0.2",
    "inquirer": "1.1.3",
    "js-yaml": "3.4.3",
    "jsonfile": "2.2.3",
    "path": "0.12.7",
    "request": "2.65.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}